{
    "version": 2,
    "builds": [
        {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
        }
    ],
    "routes": [
        {
        "src": "/api/(.*)",
        "dest": "https://secret-ravine-57617-f382e03a5810.herokuapp.com/api/$1"
        },
        {
        "src": "/(.*)",
        "dest": "frontend/build/$1"
        }
    ]
}
